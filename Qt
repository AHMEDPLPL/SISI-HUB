Tab:AddButton({
	Name = "FE Rub Animation",
	Callback = function()
		local Players = game:GetService("Players")
		local RunService = game:GetService("RunService")
		local player = Players.LocalPlayer
		local speed = 1
		local bangAnim = Instance.new("Animation")
		bangAnim.AnimationId = "rbxassetid://55816634"
		local bang = player.Character:FindFirstChildOfClass('Humanoid'):LoadAnimation(bangAnim)
		bang:Play()
		bang:AdjustSpeed(speed)

		local bangLoop = RunService.Stepped:Connect(function()
			wait(1)
			bang:Play()
		end)

		local messages = {
			"êœ°á´œá´„á´‹ Êá´‡êœ±!", "êž®á´› êœ°á´‡á´‡ÊŸêœ± êœ±á´ É¢á´á´á´…...", "á´Êœ Êá´‡á´€Êœ...", 
			"Ê™á´á´œá´œá´›á´€ Ê™á´œêœ±êœ±!", "á´€ÊœÊœ êœ±Êœlá´›!!!!!", 
			"Ê™á´á´œá´›á´›á´€ Ê™á´œêœ±á´› á´Ê êœ°á´€á´›á´€êœ±êœ± á´„á´á´„á´‹!", "* á´„á´œá´êœ± á´€ÊŸá´á´› *"
		}
		for i, msg in ipairs(messages) do
			chat(msg)
			wait(5)
			bang:AdjustSpeed(speed + (i * 0.1))
		end

		bang:AdjustSpeed(0)
		wait(4)
		player.Character.Humanoid.Health = 0

		player.Character:FindFirstChildOfClass('Humanoid').Died:Connect(function()
			if bangLoop then
				bangLoop:Disconnect()
				bang:Stop()
				bangAnim:Destroy()
			end
		end)
	end
})

function chat(msg)
    if game:GetService("TextChatService").ChatVersion == Enum.ChatVersion.TextChatService then
        game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync(msg)
    else
        game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(msg, "All")
    end
end

