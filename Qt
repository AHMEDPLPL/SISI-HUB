                    v.Anchored = true
                end
            end
            for i,v in pairs(character:GetChildren()) do
                if v:IsA("Tool") then v.Parent = workspace end
            end
            character:BreakJoints()
            plr.CharacterAdded:Wait()
            character = plr.Character
            end
            
