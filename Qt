local ToolPlr = Window:MakeTab({
	Name = "Tools",
	Icon = "rbxassetid://6034744057",
	PremiumOnly = false --- Set true, if you want to this tab was only for premium people, or false to all could use this.
})



ToolPlr:AddLabel("Beta Version")
local Section = ToolPlr:AddSection({
	Name = "Tool Stats"
})

ToolPlr:AddTextbox({
	Name = "Tool StatName",
	Default = "",
	TextDisappear = true,
	Callback = function(VRMlols)
        OrionLib:MakeNotification({
	Name = "Tutorial",
	Content = "1) You need to write statname that is in modulescript or localscript, which is function or table. Use for example dark dex and view every script in tool. 2) After finding from darkdex, you need to write here tableARG name, example CurrentAmmo or MaxAmmo. Now it's done, you can change number of table. Enjoy.",
	Image = "rbxassetid://6023426945",
	Time = 25
})
		ToolStatName = VRMlols
	end	  
})

ToolPlr:AddTextbox({
	Name = "Tool NumberStat Debug.GetRegistery() Method",
	Default = "",
	TextDisappear = true,
	Callback = function(GetRegd)
        local numbergetreg = tonumber(GetRegd)
        for i,v in pairs(debug.getregistry()) do
    if type(v) == "function" then 
       local abc = debug.getupvalues(v)
             for a,b in next, abc do
                 if type(b) == "table" then
                    if b[ToolStatName] then
                    b[ToolStatName] = numbergetreg
                 end
              end
           end
        end
     end
	end	  
})

ToolPlr:AddTextbox({
	Name = "Tool NumberStat GetGc() Method",
	Default = "",
	TextDisappear = true,
	Callback = function(gcd)
        local numbergc = tonumber(gcd)
        for i,v in next, getgc(true) do --- true means that will work, if false then not work.
    if type(v) == "function" then
        local abc = debug.getupvalues(v)
             for a,b in next, abc do
                 if type(b) == "table" and rawget(b, ToolStatName) then
                 rawset(b, ToolStatName, numbergc)
end
end
    end
end
	end	  
})

local Section = ToolPlr:AddSection({
	Name = "Tools"
})

ToolPlr:AddButton({
	Name = "Invisible Tools",
	Callback = function()
	    game.StarterGui:SetCore("SendNotification", {Title = "Reset Tutorial.", Text = "If you want to stop the invisible tools, then just reset character.", Duration = 10,})
      	local Char = game.Players.LocalPlayer.Character
	local touched = false
	local tpdback = false
	local box = Instance.new('Part')
	box.Anchored = true
	box.CanCollide = true
	box.Size = Vector3.new(10,1,10)
	box.Position = Vector3.new(0,10000,0)
	box.Parent = workspace
	local boxTouched = box.Touched:connect(function(part)
		if (part.Parent.Name == game.Players.LocalPlayer.Name) then
			if touched == false then
				touched = true
				local function apply()
					local no = Char.HumanoidRootPart:Clone()
					wait(.25)
					Char.HumanoidRootPart:Destroy()
					no.Parent = Char
					Char:MoveTo(loc)
					touched = false
				end
				if Char then
					apply()
				end
			end
		end
	end)
	repeat wait() until Char
	local cleanUp
	cleanUp = game.Players.LocalPlayer.CharacterAdded:connect(function(char)
		boxTouched:Disconnect()
		box:Destroy()
		cleanUp:Disconnect()
	end)
	loc = Char.HumanoidRootPart.Position
	Char:MoveTo(box.Position + Vector3.new(0,.5,0))
  	end    
})



ToolPlr:AddButton({
	Name = "Equip Tools",
	Callback = function()
       for i,v in pairs(Players.LocalPlayer.Backpack:GetChildren()) do
		if v:IsA("Tool") then
			v.Parent = Players.LocalPlayer.Character
		end
	end
	end    
})

ToolPlr:AddButton({
	Name = "Droppable Tools",
	Callback = function()
       if speaker.Character then
		for _,obj in pairs(speaker.Character:GetChildren()) do
			if obj:IsA("Tool") then
				obj.CanBeDropped = true
			end
		end
	end
	if speaker:FindFirstChildOfClass("Backpack") then
		for _,obj in pairs(speaker:FindFirstChildOfClass("Backpack"):GetChildren()) do
			if obj:IsA("Tool") then
				obj.CanBeDropped = true
			end
		end
	end
	end    
})

ToolPlr:AddButton({
	Name = "Drop Tool",
	Callback = function()
	wait()
	for i,v in pairs(Players.LocalPlayer.Character:GetChildren()) do
		if v:IsA("Tool") then
			v.Parent = workspace
		end
	end
	end    
})

ToolPlr:AddButton({
	Name = "Drop Tools",
	Callback = function()
       for i,v in pairs(Players.LocalPlayer.Backpack:GetChildren()) do
		if v:IsA("Tool") then
			v.Parent = Players.LocalPlayer.Character
		end
	end
	wait()
	for i,v in pairs(Players.LocalPlayer.Character:GetChildren()) do
		if v:IsA("Tool") then
			v.Parent = workspace
		end
	end
	end    
})

local HttpService = game:GetService('HttpService')
local Players = game:GetService('Players')
local RunService = game:GetService('RunService')

local function Notify(Text, Duration)
    Duration = Duration or 4
    print("[NOTIFY] ("..Duration.."s): " .. Text)
end

local function LatentYield(N)
    for _ = 1, N do
        game['RobloxReplicatedStorage'].GetServerVersion:InvokeServer()
    end
end

local function RestoreTool(Tool, RootCFrame)
    if Tool and Tool:IsDescendantOf(workspace) then
        Tool.Anchored = false
        Tool.CFrame = RootCFrame
    end
end

local function DupeTools(Num)
    local LP = Players.LocalPlayer
    local Tools = {}
    local RCF  -- Root CFrame backup

    for i = 1, Num + 1 do
        local Char = LP.Character
        local Root = Char and Char:FindFirstChild('HumanoidRootPart')

        -- Wait until the Root Part exists
        while not Root do
            Char.ChildAdded:Wait()
            Root = Char:FindFirstChild('HumanoidRootPart')
        end

        if i == 1 then
            RCF = Root.CFrame  -- Save the original CFrame
        end

        LatentYield(2.0)

        -- Final step: Restore all duplicated tools
        if i > Num then
            for _, Tool in ipairs(Tools) do
                RestoreTool(Tool, RCF)
                LatentYield(0.5)
            end
            Notify('Dupe-Tools complete.', 3)
            Root.CFrame = RCF
            break
        end

        -- Transfer tools from Backpack to Character
        for _ = 1, 10 do
            for _, Tool in ipairs(LP.Backpack:GetChildren()) do
                if Tool:IsA('Tool') then
                    Tool.Parent = Char
                end
            end
            RunService.Heartbeat:Wait()
        end

        -- Randomly teleport the player to prevent interference
        Root.CFrame = CFrame.new(math.random(-30000, 30000), 500000, math.random(-30000, 30000))
        LatentYield(2.0)

        -- Anchor tools and save their handles for restoration
        Char.Head.Anchored = true
        for _, Tool in ipairs(Char:GetChildren()) do
            if Tool:IsA('Tool') and Tool:FindFirstChild('Handle') then
                Tool.Parent = workspace
                Tool.Handle.Anchored = true
                table.insert(Tools, Tool.Handle)
            end
        end

        -- Force respawn by destroying the torso
        (Char:FindFirstChild('Torso') or Char:FindFirstChild('UpperTorso')):Destroy()
        LP.CharacterAdded:Wait()
    end
end

-- Ensure physics remain smooth and tools donâ€™t sleep
settings().Physics.AllowSleep = false
settings().Physics.PhysicsEnvironmentalThrottle = Enum.EnviromentalPhysicsThrottle.Disabled

-- Keep simulation radius high
local NK = true
coroutine.wrap(function()
    while NK do
        RunService.RenderStepped:Wait()
        pcall(function()
            setsimulationradius(math.huge, math.huge)
        end)
    end
end)()

ToolPlr:AddButton({	Name = "Dupe Tools",	Callback = function()DupeTools(1)    	end    })

local RU = false

ToolPlr:AddToggle({
    Name = "Loop Tools Dupe",
    Default = false,
    Callback = function(state)
        RU = state  -- Set RU based on the toggle state
        if RU then
            repeat
                DupeTools(1)
                wait(2)
            until not RU  -- Loop until RU is false
        else
            RU = false  -- Set RU to false when toggled off
        end
    end    
})

