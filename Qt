                if v:IsA("Tool") then v.Parent = character end
            end
            for i,v in pairs(character:GetChildren()) do
                if v:IsA("Tool") then v.Parent = character end
            end
            character.HumanoidRootPart.CFrame = CFrame.new(0,9999999999,0)
