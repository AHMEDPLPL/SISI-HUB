local method = nil

Tab:AddDropdown({
    Name = "Method",
    Options = {
        "Ä›xÌÇŽá¸¿pÌlÌÄ›", "eÌxáº¡mpÌlÌe", "áº¹Ì²Ñ…Ì²áº¡Ì²á¹ƒÌ²Ñ€Ì²á¸·Ì²áº¹Ì²"
    },
    Callback = function(val)
        method = val
    end
})

function bypasstext(text, method)
    if method == 1 then
        local function addAccents(word)
            local accents = {
                a = "ÇŽ", b = "bÌ‡", c = "Ä‡", d = "dÌ", e = "Ä›", f = "fÌ‡", 
                g = "gÌ‡", h = "hÌ‡", i = "Ã­", j = "jÌ", k = "kÌ", l = "lÌ", 
                m = "á¸¿", n = "nÌ‹", o = "Å", p = "pÌ", q = "qÌ", r = "rÌ", 
                s = "sÌ", t = "tÌ‹", u = "Å«", v = "vÌ‡", w = "wÌ", x = "xÌ", 
                y = "Ã½", z = "Åº", A = "Ç", B = "BÌ‡", C = "Ä†", D = "DÌ", 
                E = "Äš", F = "FÌ‡", G = "GÌ‡", H = "HÌ‡", I = "Ã", J = "JÌ", 
                K = "KÌ", L = "LÌ", M = "á¸¾", N = "NÌ‹", O = "ÅŒ", P = "PÌ", 
                Q = "QÌ", R = "RÌ", S = "SÌ", T = "TÌ‹", U = "Åª", V = "VÌ‡", 
                W = "WÌ", X = "XÌ", Y = "Ã", Z = "Å¹"
            }

            local bypassedWord = ""
            for i = 1, #word do
                local letter = word:sub(i, i)
                if accents[letter] then
                    bypassedWord = bypassedWord .. accents[letter]
                else
                    bypassedWord = bypassedWord .. letter
                end
            end

            return bypassedWord
        end

        local function bypassString(input)
            local words = {}
            for word in string.gmatch(input, "%S+") do
                table.insert(words, addAccents(word))
            end
            return table.concat(words, " ")
        end

        return bypassString(text)
    elseif method == 2 then
        local function addAccents2(word)
            local accents2 = {
                a = "áº¡", b = "bÌ‡", c = "c", d = "dÌ", e = "e", f = "fÌ‡", 
                g = "gÌ‡", h = "hÌ‡", i = "i", j = "jÌ", k = "á¸³", l = "lÌ", 
                m = "m", n = "nÌ‹", o = "o", p = "pÌ", q = "qÌ", r = "rÌ", 
                s = "á¹£", t = "t", u = "á»¥", v = "vÌ‡", w = "wÌ", x = "xÌ", 
                y = "y", z = "z", A = "áº ", B = "BÌ‡", C = "C", D = "DÌ", 
                E = "E", F = "FÌ‡", G = "GÌ‡", H = "HÌ‡", I = "I", J = "JÌ", 
                K = "KÌ", L = "LÌ", M = "M", N = "N", O = "O", P = "PÌ", 
                Q = "QÌ", R = "RÌ", S = "á¹¢", T = "T", U = "á»¤", V = "VÌ‡", 
                W = "WÌ", X = "XÌ", Y = "Y", Z = "Z"
            }

            local bypassedWord = ""
            for i = 1, #word do
                local letter = word:sub(i, i)
                if accents2[letter] then
                    bypassedWord = bypassedWord .. accents2[letter]
                else
                    bypassedWord = bypassedWord .. letter
                end
            end

            return bypassedWord
        end

        local function bypassString(input)
            local words = {}
            for word in string.gmatch(input, "%S+") do
                table.insert(words, addAccents2(word))
            end
            return table.concat(words, " ")
        end

        return bypassString(text)
    elseif method == 3 then
        local function addAccents3(word)
            local accents3 = {
                a = "áº¡Ì²", b = "á¸…Ì²", c = "ÑÌ²", d = "á¸Ì²", e = "áº¹Ì²", f = "fÌ²", 
                g = "É¡Ì²", h = "á¸¥Ì²", i = "á»‹Ì²", j = "Ñ˜Ì²", k = "á¸³Ì²", l = "á¸·Ì²", 
                m = "á¹ƒÌ²", n = "á¹‡Ì²", o = "á»Ì²", p = "Ñ€Ì²", q = "qÌ²", r = "á¹›Ì²", 
                s = "á¹£Ì²", t = "á¹­Ì²", u = "á»¥Ì²", v = "á¹¿Ì²", w = "áº‰Ì²", x = "Ñ…Ì²", 
                y = "á»µÌ²", z = "áº“Ì²", A = "áº Ì²", B = "á¸„Ì²", C = "Ð¡Ì²", D = "á¸ŒÌ²", 
                E = "áº¸Ì²", F = "FÌ²", G = "êž¬Ì²", H = "á¸¤Ì²", I = "á»ŠÌ²", J = "ÐˆÌ²", 
                K = "á¸²Ì²", L = "á¸¶Ì²", M = "á¹‚Ì²", N = "á¹†Ì²", O = "á»ŒÌ²", P = "Ð Ì²", 
                Q = "QÌ²", R = "á¹šÌ²", S = "á¹¢Ì²", T = "á¹¬Ì²", U = "á»¤Ì²", V = "á¹¾Ì²", 
                W = "áºˆÌ²", X = "Ð¥Ì²", Y = "á»´Ì²", Z = "áº’Ì²"
            }

            local bypassedWord = ""
            for i = 1, #word do
                local letter = word:sub(i, i)
                if accents3[letter] then
                    bypassedWord = bypassedWord .. accents3[letter]
                else
                    bypassedWord = bypassedWord .. letter
                end
            end

            return bypassedWord
        end

        local function bypassString(input)
            local words = {}
            for word in string.gmatch(input, "%S+") do
                table.insert(words, addAccents3(word))
            end
            return table.concat(words, " ")
        end

        return bypassString(text)
    end
end

